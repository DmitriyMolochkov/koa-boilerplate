{
  "name": "fastify-learning",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "main": "build/index.js",
  "imports": {
    "#config": "./config"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "ts-node --preferTsExts src/index.ts",
    "deploy": "npm run build-js && npm run clean-build && ts-node --swc --preferTsExts src/index.ts",
    "build-js": "tsc --build --force ./src",
    "clean-build": "rimraf ./build && tsc --build ./src --clean",
    "start-js": "node --experimental-specifier-resolution=node ./build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.(js|jsx|mjs|ts|tsx)": [
      "eslint --fix --max-warnings=0 --no-inline-config"
    ]
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "http-status-code": "^2.1.0",
    "joi": "^17.9.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@swc/core": "^1.3.46",
    "@swc/helpers": "^0.5.0",
    "@types/eslint": "^8.37.0",
    "@types/lint-staged": "^13.2.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
