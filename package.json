{
  "name": "fastify-learning",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "main": "build/index.js",
  "imports": {
    "#class-validator": "./src/class-validator"
  },
  "scripts": {
    "nodemon": "nodemon",
    "prepare": "husky install",
    "start": "set NODE_ENV=development&& ts-node --preferTsExts src/index.ts",
    "deploy": "set NODE_ENV=production&&npm run build-js && npm run clean-build && ts-node --swc --preferTsExts src/index.ts",
    "build-js": "tsc --build --force tsconfig.json && tsc-alias --project tsconfig.json",
    "clean-build": "rimraf ./build && tsc --build tsconfig.json --clean",
    "start-js": "set NODE_ENV=production&& node --experimental-specifier-resolution=node ./build/index.js",
    "test-spec": "jest --config jest.spec.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.(js|cjs|msx|jsx|ts|tsx|html|vue)": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ext": "js, .cjs, mjs, jsx, ts, tsx, json, html, vue",
    "ignore": [
      "build",
      ".husky"
    ],
    "exec": "npm run start",
    "events": {
      "start": "cls || clear"
    }
  },
  "dependencies": {
    "@types/config": "^3.3.0",
    "ajv": "8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "config": "^3.3.9",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "http-status-code": "^2.1.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.5.1",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/lint-staged": "^13.2.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.4"
  }
}
